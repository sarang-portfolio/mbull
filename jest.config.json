{
    "preset": "ts-jest",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
        "./setup-env.js"
    ],
    "testMatch": [
        "**/?(*.)+(spec).ts"
    ],
    "transform": {
        "^.+\\.ts$": "ts-jest"
    },
    "resetMocks": true,
    "clearMocks": true,
    "globals": {
        "ts-jest": {
            "isolatedModules": true
        }
    }
}